{
  "name": "asm-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:home": "ng serve home  --allowed-hosts true",
    "start:home:prod": "ng serve home --configuration production --allowed-hosts true",
    "start:home:aspire": "run-script-os",
    "start:home:aspire:default": "echo $PORT && ng serve home --port $PORT",
    "start:home:aspire:win32": "ng serve home --port %PORT%",
    "build:home": "ng build home --configuration production",
    "lint": "ng lint",
    "sheriff:check": "sheriff verify",
    "sheriff:list": "sheriff list "
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular-architects/ngrx-toolkit": "^20.5.0",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@ng-icons/core": "^33.0.0",
    "@ng-icons/solar-icons": "^33.0.0",
    "@ngneat/dialog": "^5.2.0",
    "@ngneat/hotkeys": "^4.1.0",
    "@ngrx/operators": "^20.1.0",
    "@ngrx/router-store": "^20.1.0",
    "@ngrx/signals": "^20.1.0",
    "@ngrx/store": "^20.1.0",
    "@ngrx/store-devtools": "^20.1.0",
    "animate.css": "^4.1.1",
    "cally": "^0.8.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.0",
    "@catppuccin/daisyui": "^2.1.1",
    "@softarc/eslint-plugin-sheriff": "^0.19.6",
    "@softarc/sheriff-core": "^0.19.6",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.19",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "angular-eslint": "21.0.1",
    "daisyui": "^5.5.8",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.3",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "run-script-os": "^1.1.6",
    "tailwindcss": "^4.1.12",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.46.4",
    "vitest": "^4.0.8"
  }
}
